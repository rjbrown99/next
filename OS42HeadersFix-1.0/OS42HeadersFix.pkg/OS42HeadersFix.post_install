#!/bin/sh

#set-up the environment
PATH=/bin:/usr/bin:/usr/ucb:/etc

stamppath="/NextDeveloper/Headers"
stamp=".OS42HeadersFix"

#cleanup the screen
echo

# install a stamp to prevent multiple installations and loss of original backup
echo "    Instaling $stamp presence stamp to prevent multiple installations..."
touch "$stamppath"/"$stamp"

# we are done
echo "    Package installation is complete."
exit 0